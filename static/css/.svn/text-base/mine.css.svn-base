/*order img */
.order-on{
	display: inline-block;
	width: 0.6933333333rem;
	height: 0.6933333333rem;
}
.onwithdraw{
	background: url(../imgs/icons/withdraw-on.png) no-repeat center;
	background-size:0.693333rem;
}
.order-min{
	display: inline-block;
	width: 0.5333333333rem;
	height: 0.5333333333rem;
	margin-left: 0.08rem;
	
}
.icon-withdraw{
	background: url(../imgs/icons/withdraw-min.png) no-repeat center;
	background-size:0.533333rem;
}

/*withdraw*/
.bankmain{
	background: #ffffff;
	border-radius: 0.133333rem;
	height:2.933333rem;
	margin:0.4rem 0.453333rem;
}
.bankimg{
	float:left;
	margin:0.533333rem 0.4rem 0 0.4rem;
	background: url(../imgs/icons/banklogo.png) no-repeat center;
	width: 1.066667rem;
	height: 1.066667rem;
	background-size: 1.066667rem;
}
.bankmsg{
	float:left;
	line-height: 100%;
	margin-top:0.533333rem;
	font-size:0.453333rem;
	color:#444444;
}
.bankname{
	font-size:0.453333rem;
}
.bankcartype{
	margin-top:0.266667rem;
	font-size:0.346667rem;
	color:#999999;
}
.banknum{
	margin-top:0.56rem;
	font-size:0.4rem;
	color:#898989;
}
.banktip{
	margin:0 0.453333rem;
	margin-top:0.266667rem;
}
.banktip-icon{
	display:inline-block;
	margin-top:0.133333rem;
	background: url(../imgs/icons/icon-tip-blue.png) no-repeat center;
	width:0.266667rem;
	height:0.266667rem;
	background-size:0.266667rem;
}
.banktip-txt{
	font-size: 0.32rem;
	color: #898989;
}

.withdraw-fail {
	margin: 0.266667rem 0 0.8rem 0;
	line-height:2.933333rem;
	width: 100%;
	background: #ffffff;
	text-align: center;
	font-size: 0;
}
.fail-icon{
	display: inline-block;
	background: url(../imgs/icons/withdraw-fail.png) no-repeat center;
	width: 0.826667rem;
	height: 0.826667rem;
	background-size:0.826667rem;
	vertical-align: middle;
	margin-right:0.333333rem;
}
.fail-msg{
	font-size:0.453333rem;
	color:#fc8800;
	vertical-align: middle;
}

.withdraw-form{
	padding:0 0.4533333333rem;
    background: #ffffff;
    border-bottom: 1px solid #DEDEDE;
    color: #444444;
    font-size: 0.4rem;
}

.withdraw-form li{
	display: box;			  /* OLD - Android 4.4- */
	display: -webkit-box;	  /* OLD - iOS 6-, Safari 3.1-6 */
	display: -webkit-flex;	 /* NEW - Chrome */
	display: flex;
	-webkit-align-content:flex-start;
	        align-content:flex-start;
	height: 1.1733333333rem;
	line-height: 1.1733333333rem;
	border-top: 1px solid #57bff5;
}
.withdraw-form li:first-child,.withdraw-form li.noborder{
	border-top: none;
}
.withdraw-form li.noborder input{
	padding-left: 0.266667rem;
}
.withdraw-form li input{
	font-size: 0.4rem;
	color: #B5B5B5;
	border: none;
    padding: 0;
    margin-bottom: 0;
}

.withdraw-formlist {
	padding:0 0 0 0.4533333333rem;
    background: #ffffff;
    border-bottom: 1px solid #DEDEDE;
    color: #444444;
    font-size: 0.4rem;	
}

.withdraw-formlist li{
	border-top: 1px solid #dedede;
}
.bank-listicon{
	display: inline-block;
	background:url(../imgs/icons/bankicon.png) center center no-repeat;
	width:1.466667rem;
	height: 1.173333rem;
	background-size:0.426667rem;
	padding-right:0.453333rem;

}
.withdraw-bluebg{
	background: #0ea3f0;
	padding-bottom:0.533333rem;
}
.withdraw-bluebg li{
	display: flex;
	color: #ffffff;
}
.withdraw-name{
	flex: 1;
}
.withdraw-tip{
	margin-top:0.4rem;
	margin-bottom:1.2rem;
	margin-left:0.453333rem;
	font-size:0.346667rem;
	color:#898989;
}
.withdraw-safe{
	font-size: 0;
	text-align: center;
	line-height:1.2rem;
}
.withdraw-safeicon{
	display:inline-block;
	margin-right:0.106667rem;
	background: url(../imgs/icons/icon-safe.png) no-repeat;
	width: 0.266667rem;
	height:0.32rem;
	background-size:0.266667rem;
	vertical-align: middle;
}
.withdraw-safetxt{
	font-size:0.32rem;
	color:#fc8800;
	vertical-align: middle;
}
.withdraw-txt{
	flex: 1;
	text-align: right;
}
.bill-list{
	background: #ffffff;
}
.bill-list li{
	position: relative;
	font-size: 0;
	line-height: 1;
	border-top: 1px solid #DEDEDE;
	padding:0.506667rem 0.453333rem;
}
.bill-list li:first-child{
	border-top: none;
}
.bill-list a{
	color: #898989;
	font-size:0.346667rem ;
}
.bill-type{
	font-size:0.4rem;
	color:#444444;
	display: block;
	height:0.4rem;
	overflow:hidden;
	margin-right:0.666667rem;
}
.bill-remarks{
	display: block;
	margin-top:0.266667rem;
	height:0.346667rem;
	overflow:hidden;
	margin-right:0.666667rem;
}
.arrow-gray{
	position: absolute;
	top:0.8rem ;
	right:0.453333rem;
	 background: url(../imgs/icons/arrow-gray.png) no-repeat center;
	 width: 0.16rem;
	 height: 0.3066666667rem;
	 display: inline-block;
	 margin-left: 0.133333rem;
	 background-size:0.16rem;
}