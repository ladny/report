.user-header{
	padding:0.3866666667rem 0.4533333333rem 0.5066666667rem 0.4533333333rem;
	background-color: #0ea3f0;
	height:3.2rem;
    overflow: hidden;
    position: relative;
}
.setting{
	position: absolute;
	right: 0.4533333333rem;
	top:0.3733333333rem;
	background: url(../imgs/icons/Settings.png) no-repeat;
	background-size:0.506667rem;
	width: 0.5066666667rem;
	height: 0.5066666667rem;
}
.arrow-gray{
	 background: url(../imgs/icons/arrow-gray.png) no-repeat;
	 background-size:0.16rem;
	 width: 0.16rem;
	 height: 0.3066666667rem;
	 display: inline-block;
	 margin-left: 0.2666666667rem;
}
.user-title-name{
	float: left;
	color: #444444;
	font-size: 0.4rem;
}
.user-title-sub{
	color: #999999;
	font-size: 0.4rem;
}
.head-info{
	margin-top: 0.4rem;
	display: block;
	width: 100%;
}
.head-img{
	margin: 0 0.4rem 0 0;
	width: 1.8133333333rem; 
	height:1.8133333333rem;     
	border-radius:50%;
	border: 1px solid #fff;
	display: inline-block;
}
.head-img img{ 
	width: 100%;
	height: 100%;
}
.my-person-info{
	vertical-align:top;
	display: inline-block;	
	margin-top:0.266667rem;
}
.my-head-name{
	color: #fff;
	font-size: 0.4533333333rem;
	margin-bottom: 0.2666666667rem;
}
.my-head-signature{
	color: #fff;
	font-size: 0.3466666667rem;
}
.my-head-arrow{
	position: absolute;
	right: 0.4533333333rem;
	top: 1.6rem;
	display: inline-block;
	vertical-align: 100%;
	background: url(../imgs/icons/arrow-white.png) no-repeat;
	background-size:0.213333rem;
	width: 0.2133333333rem;
	height: 0.4rem;
}

.user-order{
	background: #fff;
	height: 3.7333333333rem;
	border-bottom: 1px solid #DEDEDE;
}
.user-order-head{
	height: 1.32rem;
	line-height: 1.32rem;
	border-bottom: 1px solid #DEDEDE;
	padding-right: 0.4533333333rem;
	margin-left: 0.4533333333rem;
	
}
.user-order-content{
	padding-top: 0.2933333333rem;
}
.user-order-two{
	float: left;
	width: 50%;
	text-align: center;
}
.user-order-three{
	float: left;
	width: 33%;
	text-align: center;
}

.assess-img{
	background: url(../imgs/my-gu.png) no-repeat;
}
.loan-img{
	background: url(../imgs/my-dai.png) no-repeat;
}
.qiang-img{
	background: url(../imgs/my-qiang.png) no-repeat;
}
.orderimg{
	width: 1.2rem;
	height: 1.2rem; 
	display: block;
	margin: 0 auto;
	background-size:1.2rem;
}
.order-name{
	font-size: 0.4rem;
	color: #444444;
	line-height: 0.8rem;
}

.user-assets{
	margin-top: 0.2666666667rem;
	height: 4.88rem;
	border: 1px solid #DEDEDE;
	background: #FAFAFA;
}
.user-assets ul{
	background: #fff;
}
.user-assets li{
	height: 1.3333333333rem;
	line-height: 1.3333333333rem;
	background: #fff;
	margin-left:0.4533333333rem;
	padding-right:0.4533333333rem;
}
.user-assets-head{
	 border-bottom: 1px solid #DEDEDE;
}
.user-order-head a{
	display:block;
	width:inherit;
	height:inherit;
}

.btn-withdraw{
	float: right;
	margin-top:0.3333333333rem;
	width: 2.1333333333rem;
	height: 0.6666666667rem;
	line-height: 150%;
	border: 1px solid #0EA3F0;
	color: #0EA3F0;
	background: #fff;
	border-radius: 0.0666666667rem;
	font-size: 0.4rem;
	text-align: center;
}
.user-assets-return{
	height: 2.1866666667rem;
}
.user-assets-return a{
	display: inline-block;
	text-align: center;
	height:0.96rem;
	line-height:100%;
	margin-top:0.6rem;
	width: 50%;
	border-left: 1px solid #DEDEDE;
	margin-left: -3px;
}
.user-assets-num{
	font-size: 0.4rem;
	color: #FC3B00;
}
.user-assets-name{
	font-size: 0.3466666667rem;
	color: #444444;
}
.user-assets-line{
	border-left: 1px solid #DEDEDE;
	height: 0.9733333333rem;
}

.user-link{
	margin-top: 0.2666666667rem;
	padding-left: 0.4533333333rem;
	height: 1.1866666667rem;
	line-height: 1.1866666667rem;
	border-bottom: 1px solid #DEDEDE;
	padding-right: 0.4666666667rem;
	background: #fff;
}
.user-link a{
	display:block;
	width:inherit;
	height:inherit;
}

.mask{
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: #000;
	-moz-opacity: 0.75;
	opacity: 0.75;
	fliter: alpha(opacity=75%);
	z-index: 100;
}
.pupView{
	z-index: 101;
	position: fixed;
	left: 50%;
	top: 50%;
	margin-left: -3.3333333333rem;
	margin-top: -4.6666666667rem;
	width: 6.6666666667rem;
	height:9.3333333333rem;
}
.pupimg{
	background:url(../imgs/my-unlogin.png) no-repeat;
	width: 7.5733333333rem;
	height: 6.04rem;
	display: block;
	margin-left: -0.4533333333rem;
}
.pupView-btn{
	display: block;
	margin-top: 0.5333333333rem;
	width: 6.6666666667rem;
	height: 1.1733333333rem;
	line-height: 1.1733333333rem;
	background: #FC8800;
	color: #fff;
	font-size: 0.48rem;
	border-radius: 0.1333333333rem;
	text-align: center;
}
.pupView-btn:hover{
	color: #fff;
}
.pupView-reg{
	display: block;
	margin-top: 0.6666666667rem;
	text-align: center;
	font-size: 0.4rem;
	color: #29A1C4;
}
.pupView-reg:hover{
	color: #29A1C4;
}
